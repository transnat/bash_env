#!/usr/bin/env bash
xrandr \
	--output eDP-1 --off    \
	--output HDMI-1         \
        --mode 1920x1200    \
        --pos 0x300         \
        --rotate normal     \
	--output DP-1 --off     \
	--output DP-2 --off     \
	--output DP-3 --off     \
	--output DP-4 --off     \
	--output DP-3-1 --off   \
	--output DP-3-2 --off   \
	--output DP-3-3 --off 
