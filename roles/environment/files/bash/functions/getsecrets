#!/usr/bin/env bash
#-*- coding:utf-8 -*-
# Export encrypted environment variables to bash session.
getsecrets() {
    if lsusb | grep --quiet --ignore-case 'yubikey'; then
        for LINE in $(gpg --decrypt "${SECRETS}" 2> /dev/null | grep -v "^#"); do
            export "${LINE?}"
        done
    fi
}
